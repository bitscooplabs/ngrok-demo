{
  "version": "1.0",
  "url": "http://localhost:5858",

  "endpoints": {
    "Posts": {
      "route": "/posts",
      "single": {
        "route": "/posts/{{ identifier }}"
      },
      "model": {
        "key": "id",
        "fields": {
          "id": "integer",
          "title": "string",
          "userId": "integer"
        }
      }
    },
    "Users": {
      "route": "/users",
      "single": {
        "route": "/users/{{ identifier }}"
      },
      "model": {
        "key": "id",
        "fields": {
          "id": "integer",
          "name": "string",
          "username": "string",
          "email": "email"
        }
      }
    }
  }
}
